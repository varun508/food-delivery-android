<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.screens.GetStartedActivity">


    <com.trihkfoods.main.ui.widgets.viewpager.AutoSlidingViewPager
        android:id="@+id/autoSlidingViewPagerAgs"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <com.github.mmin18.widget.RealtimeBlurView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="250dp"
        app:layout_constraintBottom_toBottomOf="@+id/autoSlidingViewPagerAgs"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:realtimeBlurRadius="10dp" />


</androidx.constraintlayout.widget.ConstraintLayout>